{
    "name": "fetchkit",
    "version": "0.1.0",
    "description": "A frontend-agnostic, vanilla JavaScript-first library for fetching, updating, and managing asynchronous data",
    "main": "dist/fetchkit.js",
    "module": "dist/fetchkit.mjs",
    "types": "dist/types/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/fetchkit.mjs",
        "require": "./dist/fetchkit.js",
        "types": "./dist/types/index.d.ts"
      }
    },
    "files": [
      "dist"
    ],
    "scripts": {
      "dev": "vite",
      "build": "vite build && tsc --emitDeclarationOnly",
      "preview": "vite preview",
      "test": "vitest run",
      "test:watch": "vitest",
      "test:coverage": "vitest run --coverage",
      "lint": "eslint src --ext .ts",
      "lint:fix": "eslint src --ext .ts --fix",
      "format": "prettier --write \"src/**/*.ts\"",
      "docs": "typedoc src/index.ts"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/koficodedat/fetch-kit.git"
    },
    "keywords": [
      "fetch",
      "api",
      "http",
      "request",
      "cache",
      "swr",
      "vanilla",
      "javascript"
    ],
    "author": "Kofi Nedjoh",
    "license": "MIT",
    "devDependencies": {
      "@typescript-eslint/eslint-plugin": "^6.9.0",
      "@typescript-eslint/parser": "^6.9.0",
      "eslint": "^8.52.0",
      "eslint-config-prettier": "^9.0.0",
      "eslint-plugin-prettier": "^5.0.1",
      "prettier": "^3.0.3",
      "typedoc": "^0.25.3",
      "typescript": "^5.2.2",
      "vite": "^4.5.0",
      "vitest": "^0.34.6",
      "@vitest/coverage-v8": "^0.34.6"
    },
    "engines": {
      "node": ">=16.0.0"
    }
  }